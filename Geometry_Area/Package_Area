package sunbeam.geometry;
import sunbeam.geometry.dim2.Circle;
import sunbeam.geometry.dim3.Cylinder;
import sunbeam.geometry.dim3.Box;


public class Package_Area {

	public static void main(String[] args) {
		Circle cir=new Circle(4.4);
		System.out.println("Circle_Area:"+cir.Area());
		Cylinder cyl=new Cylinder(4.5,2.3);
		System.out.println("Cylinder_Area:"+cyl.Area());
		Box b=new Box(4.6,2.4,5.6);
		System.out.println("Box_Area:"+b.Area());
	}

}

