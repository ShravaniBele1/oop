#include<iostream>
using namespace std;

int main(){
    float num, i;
   
    cout << "Enter number: ";
    cin >> num;
    cout << "Multiplication table of " << num << " is:" << endl;
    
    for(i = 1; i <= 10; i++)
       cout << num << " * " << i << " = " << num * i << endl;
    
    return 0;
}
